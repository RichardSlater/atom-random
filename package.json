  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "chance": "^0.8.0"
  },
  "devDependencies": {
    "minimist": "^1.2.0"
  }
}
