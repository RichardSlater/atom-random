{
  "name": "random",
  "main": "./lib/random",
  "version": "0.0.1",
  "description": "Generates several random data types, for use as passwords or as test data.",
  "keywords": [],
  "activationCommands": {
    "atom-workspace": [
      "random:reseed",
      "random:string",
      "random:guid",
      "random:integer",
      "random:boolean",
      "random:character",
      "random:floating",
      "random:natural",
      "random:paragraph"
    ]
  },
